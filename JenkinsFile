pipeline {
    // master executor should be set to 0
    agent any
    stages {
        stage("Start Grid") {
            steps {
                //sh
                bat "docker-compose up --no-color -d hub chrome"
            }
        }
        stage ("Run Test") {
          steps{
                bat "docker-compose up search-module book-flight"
          }
        }
        stage("Bring Grid Down") {
            steps {
                //sh
                bat "docker-compose down"
            }
        }
    }
}
